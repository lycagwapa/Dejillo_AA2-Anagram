<!DOCTYPE html>
<html>
<head>
    <title>Anagram</title>
</head>
<body>
<script>

let input1 = prompt("Enter the first word/phrase:").toLowerCase();
let input2 = prompt("Enter the second word/phrase:").toLowerCase();

function anagram(input1, input2) {
    // Remove all non-alphabetic characters and spaces from the inputs
    let cleanInput1 = input1.replace(/[^a-z]/g, "").split("").sort().join("");
    let cleanInput2 = input2.replace(/[^a-z]/g, "").split("").sort().join("");

    if (cleanInput1 === cleanInput2) {
        return "The words/phrases are anagrams!";
    } else {
        return "The words/phrases are not anagrams.";
    }
}

let output = anagram(input1, input2);
alert(output);
</script>
</body>
</html>
